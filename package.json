{
  "name": "muni-next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "predeploy": "npm run build && npm run export",
    "deploy": "firebase deploy",
    "deploy:hosting": "firebase deploy --only hosting",
    "deploy:functions": "cd functions && npm run deploy",
    "start": "next start",
    "preview": "serve out",
    "lint": "next lint",
    "emulate": "firebase emulators:start --import=./emulator-data --export-on-exit",
    "build:functions": "cd functions && npm run build",
    "postbuild": "cp firebase.json out\\ && cp .firebaserc out\\",
    "serve": "npm run build && npm run export && firebase emulators:start",
    "serve:dev": "next dev",
    "emulate:full": "concurrently \"npm run dev\" \"firebase emulators:start --import=./emulator-data --export-on-exit\"",
    "emulate:dev": "firebase emulators:start --only functions,firestore,auth, storage --config firebase-emulators.json"
  },
  "dependencies": {
    "bootstrap": "^5.3.6",
    "firebase": "^11.9.1",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^5.0.0",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.0.0",
    "serve": "^14.2.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "dotenv": "^16.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
